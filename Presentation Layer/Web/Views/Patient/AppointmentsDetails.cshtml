<script>
    // Modal functions
    function rescheduleAppointment() {
        document.getElementById('reschedule-modal').classList.remove('hidden');
    }

    function cancelAppointment() {
        document.getElementById('cancel-modal').classList.remove('hidden');
    }

    function closeModal(modalId) {
        document.getElementById(modalId).classList.add('hidden');
    }

    function confirmReschedule() {
        closeModal('reschedule-modal');
        // Store appointment data for rescheduling
        localStorage.setItem('rescheduleAppointment', JSON.stringify({
            id: 'APT-2025-001',
            specialty: 'cardiologia',
            doctor: 'Dr. Carlos Mendoza'
        }));
        // Redirect to calendar selection
        window.location.href = 'book_appointment_calendar.html';
    }

    function confirmCancel() {
        closeModal('cancel-modal');
        // Update appointment status
        updateAppointmentStatus('cancelled');
        // Show success message
        showToast('Cita cancelada correctamente', 'success');
        // Redirect to history after delay
        setTimeout(() => {
            window.location.href = 'appointment_history.html';
        }, 2000);
    }

    function updateAppointmentStatus(status) {
        // Update appointment status in localStorage or send to API
        const appointments = JSON.parse(localStorage.getItem('appointments') || '[]');
        const appointmentIndex = appointments.findIndex(apt => apt.id === 'APT-2025-001');
        if (appointmentIndex !== -1) {
            appointments[appointmentIndex].status = status;
            localStorage.setItem('appointments', JSON.stringify(appointments));
        }
    }

    function showToast(message, type = 'info') {
        const toast = document.createElement('div');
        toast.className = `toast fixed top-4 right-4 z-50 ${type === 'success' ? 'bg-success-50 text-success border-success-200' : 'bg-primary-50 text-primary border-primary-200'}`;
        toast.innerHTML = `
            <div class="flex items-center">
                <svg class="w-5 h-5 mr-2" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                </svg>
                ${message}
            </div>
        `;
        document.body.appendChild(toast);

        setTimeout(() => {
            toast.remove();
        }, 3000);
    }

    function toggleChat() {
        alert('Chat de ayuda - Funcionalidad próximamente disponible');
    }

    document.addEventListener('click', function(event) {
        const modals = ['reschedule-modal', 'cancel-modal'];
        modals.forEach(modalId => {
            const modal = document.getElementById(modalId);
            if (event.target === modal) {
                closeModal(modalId);
            }
        });
    });

    document.addEventListener('DOMContentLoaded', function() {
        // Load appointment data from localStorage or API
        loadAppointmentDetails();
    });

    function loadAppointmentDetails() {
        console.log('Appointment details loaded');
    }
</script>
